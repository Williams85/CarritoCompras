@model List<CarritoCompras.Agente.VS.MedioPagoEntidad>
@{
    ViewBag.Title = "_ResultadoMediosPago";
}
@if (Model != null && Model.Count > 0)
{
    <legend class="col-form-label col-sm-2 pt-0">Medios de Pago</legend>
    <div class="col-sm-10">
        @foreach (var item in Model)
        {
        <div class="form-check">
            <input class="form-check-input" type="radio" name="gridRadios" id="@item.Cod_MedioPago" value="@item.Nom_MedioPago">
            <label class="form-check-label" for="gridRadios1">
                @item.Nom_MedioPago
            </label>
        </div>            
        }


    </div>

}

