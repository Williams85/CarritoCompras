                <binding name="BasicHttpBinding_IVentaService" />
                <binding name="BasicHttpBinding_IUsuarioService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:51149/VentaService.svc" binding="basicHttpBinding"
